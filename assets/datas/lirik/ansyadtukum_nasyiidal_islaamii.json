{
    "verses": [
      {
        "arabic": "أَنْشَدْتُكُمْ نَشِيْدَ الْإِسْلَامِي",
        "romaji": "Ansyadtukum nasyiidal Islaamii",
        "translation": "Aku bernasyid di hadapan kalian nasyid yang Islami."
      },
      {
        "arabic": "أَرْجُو بِهِ لِحُبِّ الْبَشِيْرِ",
        "romaji": "Arjuu bihi lihubbil basyiiri",
        "translation": "Aku berharap dengan nasib ini menambahkan kecintaanku kepada Sang Pencipta Manusia."
      },
      {
        "arabic": "ثَبِّتْ فُؤَادِيْ يَا رَبِّ",
        "romaji": "Tsabbit fuaadii yaa robbi",
        "translation": "Tetaplah hatiku, Wahai Tuhanku."
      },
      {
        "arabic": "بِمَدْحِ طٰهٰ الْمُخْتَارِ",
        "romaji": "Bimadhi thoohal muktaari",
        "translation": "Dengan memuji Tuhan yang terpilih."
      },
      {
        "arabic": "أَنْشَدْتُكُمْ مَعَ حُضُوْرِ الْقَلْبِ",
        "romaji": "Ansyadtukum ma'a khuduuril qolbi",
        "translation": "Aku menasibkan untuk kalian beserta hati yang hadir khusyuk."
      },
      {
        "arabic": "يُزِيْلُ هُمُوْمَ الضَّمِيْرِ",
        "romaji": "Yuziilu humuumad dhamiiri",
        "translation": "Yang mudah-mudahan akan menghilangkan luka kita yang timbul dalam hati."
      },
      {
        "arabic": "ثَبِّتْ فُؤَادِيْ يَا رَبِّ",
        "romaji": "Tsabbit fuaadii yaa robbi",
        "translation": "Tetapkanlah hatiku, Wahai Tuhanku."
      },
      {
        "arabic": "بِمَدْحِ طٰهٰ الْمُخْتَارِ",
        "romaji": "Bimadhi thoohal muktaari",
        "translation": "Dengan memuji Tuhan yang terpilih."
      },
      {
        "arabic": "أَنْشَدْتُكُمْ تَمَكُّنَ النَّفْسِ",
        "romaji": "Ansyadtukum tamakkunan nafsi",
        "translation": "Saya berharap jadilah diri sendiri."
      },
      {
        "arabic": "أُشِيْقُ نُفُوْسَ الْحَضِيْرِ",
        "romaji": "Usyiiqu nufuusal hadhiiri",
        "translation": "Di setiap hembusan nafas."
      },
      {
        "arabic": "ثَبِّتْ فُؤَادِيْ يَا رَبِّ",
        "romaji": "Tsabbit fuadi yaa robbi",
        "translation": "Tetapkanlah hatiku, Wahai Tuhanku."
      },
      {
        "arabic": "بِمَدْحِ طٰهٰ الْمُخْتَارِ",
        "romaji": "Bimadhi thoohal muktari",
        "translation": "Dengan memuji Tuhan yang terpilih."
      },
      {
        "arabic": "صَلَاةُ رَبِّيْ دَائِمً",
        "romaji": "Sholaatu robbii daa-iman",
        "translation": "Semoga Sholawat dari Tuhanku selalu."
      },
      {
        "arabic": "لِلنَّبِي خَيْرِ الْبَشَرِ",
        "romaji": "Linnabii khoiril basyari",
        "translation": "Tercurahkan kepada Nabi SAW sebaik-baik manusia."
      }
    ],
    "arabicText": "أَنْشَدْتُكُمْ نَشِيْدَالْإِسْلَامِي"
  }
  